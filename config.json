{
    "target": {
        "__comment": "which datastructures will be evaluated",

        "clht_lb": "./structures/clht_lb.so",
        "clht_lf": "./structures/clht_lf.so",
        "clht_lf_res": "./structures/clht_lf_res.so",
        "cuckoo": "./structures/cuckoo.so"
    },
    "benchmarks": {
        "normal_benchmark": {
            "benchmark": "MBench_1",
            "target": [
                "clht_lb", "cuckoo"
            ],

            "__comment": "the threadnum should be a list [] - usecases",
            "threadnum": [1, 2, 4],
            "capacity": 100,
            "repetitions": 5,
            "perf": {
                "do_profiling": true,
                "events": "branch-misses,cache-misses,page-faults"
            },
            
            "__distribution_types": "[uniform, zipfian]",
            "distribution": "zipfian",
        
            "performfill": {
                "filling_factor": 0.95
            },
        
            "performquery": {
                "query_factor":   0.7,
                "success_factor": 0.5
            },
        
            "performdeletion": {
                "deletion_factor":  0.7,
                "success_factor":   0.5
            }
        }
    }
}