{
    "target": {
        "__comment": "which datastructures will be evaluated",

        "clht_lb": "./structures/clht_lb.so",
        "clht_lf": "./structures/clht_lf.so",
        "clht_lf_res": "./structures/clht_lf_res.so",
        "cuckoo": "./structures/cuckoo.so",
        "tbb": "./structures/tbb.so",
        "hot": "./structures/hot.so"
    },
    "benchmarks": {
        "normal_benchmark": {
            "benchmark": "MBench",
            "target": [
                "clht_lf"
            ],

            "__comment": "the threadnum should be a list [] - usecases",
            "threadnum": [1, 2, 4],
            "capacity": 1024,
            "repetitions": 5,
            "perf": {
                "do_profiling": false,
                "events": "branch-misses,cache-misses,page-faults"
            },
            
            "__distribution_types": "[uniform, zipfian]",
            "distribution": "zipfian",
        
            "performfill": {
                "filling_factor": 0.95
            },
        
            "performquery": {
                "query_factor":   0.7,
                "success_factor": 0.1
            },
        
            "performdeletion": {
                "deletion_factor":  0.7,
                "success_factor":   0.5
            }
        },
        "normal_ycsb": {
            "benchmark": "YCSB",
            "target": [
                "cuckoo"
            ],
            
            "repetitions": 5,
            "__comment": "to execute load phase before each workload",
            "phase_load": true,
    
            "workloads": {
                "workloada": {
                    "recordcount": 1000000,
                    "operationcount": 1000000,
                    "workload": "com.yahoo.ycsb.workloads.CoreWorkload",
    
                    "readallfields": true,
    
                    "readproportion": 0.95,
                    "updateproportion": 0.05,
                    "scanproportion": 0,
                    "insertproportion": 0,
                    "requestdistribution": "zipfian"
                },
                "workloadb": {
                    "recordcount": 1000000,
                    "operationcount": 1000000,
                    "workload": "com.yahoo.ycsb.workloads.CoreWorkload",
    
                    "readallfields": true,
    
                    "readproportion": 0.95,
                    "updateproportion": 0.05,
                    "scanproportion": 0,
                    "insertproportion": 0,
    
                    "requestdistribution": "zipfian"
                },
                "workloadc": {
                    "recordcount": 1000000,
                    "operationcount": 1000000,
                    "workload": "com.yahoo.ycsb.workloads.CoreWorkload",
    
                    "readallfields": true,
    
                    "readproportion": 1,
                    "updateproportion": 0,
                    "scanproportion": 0,
                    "insertproportion": 0,
    
                    "requestdistribution": "zipfian"
                }
            }
        }
    }
}